/system         ext4    /dev/block/bootdevice/by-name/system
/system_image   emmc    /dev/block/bootdevice/by-name/system
/vendor         ext4    /dev/block/bootdevice/by-name/vendor                     flags=backup=1;wipeingui
/vendor_image   emmc    /dev/block/bootdevice/by-name/vendor                     flags=backup=1
/data           ext4    /dev/block/bootdevice/by-name/userdata                   flags=encryptable=/dev/block/bootdevice/by-name/metadata
/cache          ext4    /dev/block/bootdevice/by-name/cache
/misc           emmc    /dev/block/bootdevice/by-name/misc
/boot           emmc    /dev/block/bootdevice/by-name/boot
/recovery       emmc    /dev/block/bootdevice/by-name/recovery                   flags=backup=1
/persist        emmc    /dev/block/bootdevice/by-name/persist                    flags=backup=1
/firmware       ext4    /dev/block/bootdevice/by-name/modem	                 flags=backup=1
/oem            ext4    /dev/block/bootdevice/by-name/oem

/external_sd    auto    /dev/block/mmcblk1p1            /dev/block/mmcblk1       flags=fsflags=utf8;display="External SDcard";storage;wipeingui;removable
/usb_otg        auto    /dev/block/sda1                 /dev/block/sda           flags=display="USB-OTG";storage;wipeingui;removable




